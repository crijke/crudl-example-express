{
  "name": "crudl-express-blog",
  "version": "0.2.0",
  "description": "crudl example express blog",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node --presets es2015,stage-2",
    "dropdb": "node ./scripts/dropdb.js",
    "initdb": "node ./scripts/initdb.js",
    "revert": "npm run dropdb && npm run initdb"
  },
  "proxy": "http://localhost:3000",
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-extra-semi": 2,
      "semi": 0,
      "react/jsx-filename-extension": 0,
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier",
      "babel"
    ],
    "env": {
      "browser": true,
      "jest": true
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "author": "vonautomatisch",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.0",
    "prettier": "^1.8.2"
  },
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.0",
    "colors": "^1.1.2",
    "express": "^4.13.4",
    "express-graphql": "^0.4.13",
    "express-paginate": "^0.2.0",
    "graphql": "^0.4.18",
    "graphql-relay": "^0.4.2",
    "mongoose": "^4.4.10",
    "mongoose-paginate": "^5.0.0",
    "validator": "^5.5.0",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "faker": "^3.1.0",
    "pow-mongoose-fixtures": "^0.3.0"
  }
}
